  "keywords": [
    "Python",
    "Tkinter",
    "URL Shortener",
    "pyshorteners",
    "pyperclip",
    "GUI",
    "Multithreading",
    "TinyURL",
    "Desktop App"
  ],
  "main": "url_shortener.py",
